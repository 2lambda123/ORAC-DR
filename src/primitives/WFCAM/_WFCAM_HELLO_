    # Load in required modules

    use Cirdr::Primitives qw(:constants :routines);
    use Astro::FITS::CFITSIO qw(:constants :longnames);
    use File::Copy;

    # Get the next frame

    my $inframe = $Frm->file;
    print STDERR "***$inframe***\n";

    # Copy it to the output directory

    my $inf = "$ENV{'ORAC_DATA_IN'}/$inframe";
    my $of = "$ENV{'ORAC_DATA_OUT'}/$inframe";
    if (-l $of) {
        unlink $of;
        copy($inf,$of);
    }


