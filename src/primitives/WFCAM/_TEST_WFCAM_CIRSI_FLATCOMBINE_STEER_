    # Number of offsets in the jitter pattern

    my $number = $Frm->hdr("NJITTER");

    # Which one is this?

    my $njit = $Frm->hdr("JITTER_I");

    # OK, set a flag.  If this is the last in the jitter pattern, then
    # flag that we've got all the ones we need.

    $Frm->uhdr("JITTER_COMPLETE",1) if ($number == $njit);
