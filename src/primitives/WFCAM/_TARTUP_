    # Only do this step if we've got the whole group here...

    if ($Frm->uhdr("JITTER_COMPLETE")) {

        # Loop for each member of the group.  Make an object mask and then
        # do the reset anomaly correction

        foreach my $frm ($Grp->allmembers) {
            my $Frm = $frm;
            _MAKE_OBJECT_MASK_ PNOP=15 THRESH=-1.5 GRID=64
            _RESET_ANOM_ MEDFILT=50 LINFILT=25
            unlink $Frm->uhdr("OPM");
        }
    }
