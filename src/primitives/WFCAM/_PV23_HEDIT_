    # Get the distortion value for this frame.

    my $pv23 = $Frm->pv2_3;

    # Now hedit the damn thing in...

    my $errmsg;
    foreach my $i (1 .. $Frm->findnsubs) {
        my $retval = cir_update_hdr($Frm->getasubframe($i)->file,"PV2_3",
            "FLOAT",$pv23,"",$errmsg);
    }
   
    
