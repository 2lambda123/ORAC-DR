# -*- perl -*-
# _APHOT_MAG_
#
# Primitive to compute aperture photometry from an image, allowing for
# aperture correction.
#

# This should only be performed on OBJECT frames.
    if ($Frm->hdr(OBSTYPE) =~ /OBJECT/) {
  
       _ESTIMATE_PSF_
       _APERTURE_CORRECTION_
       _APERTURE_PHOTOMETRY_
    }
