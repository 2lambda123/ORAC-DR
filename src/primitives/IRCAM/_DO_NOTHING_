# _DO_NOTHING_
#
# primitive to be used in the many-in-no-out scenario
#
# does nothing besides actually turn the link to the raw
# data into an actualy file
#
# requires File::Copy

# frossie@jach.hawaii.edu
    (my $in, my $out) = $Frm->inout( "_nul" );
    $Mon{'kappa_mon'}->obeyw("cmult","in=$in scalar=1 out=$out");
    $Frm->file( $out );
