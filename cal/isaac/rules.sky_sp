# The sky is slit and filter dependent, 
# so all must match (GRATENC is allowed to change)
FILTER eq $Hdr{FILTER}
SLIT eq $Hdr{SLIT}
SLITANG eq $Hdr{SLITANG}

# The exposure times must match.
EXPTIME == $Hdr{EXPTIME}

# Target readout area should be contained within calibrator frame.
STARTX <= $Hdr{STARTX}
STARTY <= $Hdr{STARTY}
WINNX >= $Hdr{WINNX}
WINNY >= $Hdr{WINNY}

# Write the oractime in the index to select the sky closest
# temporally to the current frame.
ORACTIME
